'''--------------13-----------------------------------------------------------------------------------------------------------------------------------------
-------------------------------------------------------------#flipkart announces the end year stock----------------------------------------------------------------------'''











def calculate_powers(N):
    power_1 = N
    power_2 = N ** 2
    power_3 = N ** 3
    return power_1, power_2, power_3
N = int(input())
result = calculate_powers(N)
print(*result)









'''----------------14---------------------------------------------------------------------------------------------------------------------------------------
---------------------------------------------------#aarav was a popular maths trainer-------------------------------------------------------'''










def ones_place_digit(num):
    ones_digit = num % 10
    return ones_digit
num = int(input())
result = ones_place_digit(num)
print(result)











'''-----------------15--------------------------------------------------------------------------------------------------------------------------------------
---------------------------------------------#kartik was working in the hr division of audi------------------------------------------------'''















def calculate_salary(working_hours, salary_per_day):
    total_salary = working_hours * salary_per_day
    return total_salary
working_hours = int(input())
salary_per_day = float(input())
result = calculate_salary(working_hours, salary_per_day)
print("{:.2f}".format(result)











'''---------------16----------------------------------------------------------------------------------------------------------------------------------------
-----------------------------------------------#caleb is a physicist working for DASA------------------------------------------------------------'''












import math
distance = float(input())
gravity = 9.8 
final_speed = math.sqrt(2 * distance * gravity)
print(f"{final_speed:.2f} m/s")
